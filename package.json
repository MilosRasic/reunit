{
	"name": "reunit",
	"version": "0.0.7",
	"description": "Library for unit testing React components",
	"main": "index.js",
	"scripts": {
		"prepare": "husky install",
		"test": "mocha --require src/test-helper.js src/**/*.test.js"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"author": "Miloš Rašić <milos.rasic@gmail.com>",
	"license": "MIT",
	"keywords": [
		"react",
		"testing",
		"unit testing",
		"shallow rendering"
	],
	"devDependencies": {
		"chai": "^4.3.4",
		"core-js": "^3.20.2",
		"husky": "^7.0.4",
		"jest": "^27.4.6",
		"mocha": "^9.1.3",
		"prettier": "2.5.1",
		"proxyquire": "^2.1.3",
		"react": "^17.0.2",
		"react-test-renderer": "^17.0.2",
		"sinon": "^12.0.1",
		"sinon-chai": "^3.7.0"
	},
	"peerDependencies": {
		"react": ">17.0.0",
		"react-test-renderer": ">17.0.0"
	}
}
